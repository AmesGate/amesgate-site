{
  "routes": [
    {
      "route": "/*",
      "redirect": "/",
      "statusCode": 301
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/favicon.ico"]
  },
  "globalHeaders": {
    "strict-transport-security": "max-age=31536000; includeSubDomains; preload"
  }
}
